                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        ChiTietHoaDon.MaHD, ChiTietHoaDon.MaHH, ChiTietHoaDon.SoLuong, ChiTietHoaDon.DonGia, ChiTietLoaiHang.MaLHH, HoaDon.NgayLap, HangHoa.TenHH, HangHoa.Gia, LoaiHangHoa.TenLHH, NhanVien.TenNV, 
                         ChiTietHoaDon.SoLuong * HangHoa.Gia AS ThanhTien
FROM            ChiTietHoaDon INNER JOIN
                         HangHoa ON ChiTietHoaDon.MaHH = HangHoa.MaHH INNER JOIN
                         HoaDon ON ChiTietHoaDon.MaHD = HoaDon.MaHD INNER JOIN
                         KhachHang ON HoaDon.MaKH = KhachHang.MaKH INNER JOIN
                         LoaiHangHoa ON HangHoa.MaLHH = LoaiHangHoa.MaLHH INNER JOIN
                         GianHang ON LoaiHangHoa.MaGH = GianHang.MaGH INNER JOIN
                         ChiTietLoaiHang ON LoaiHangHoa.MaLHH = ChiTietLoaiHang.MaLHH INNER JOIN
                         NhaCungCap ON ChiTietLoaiHang.MaNCC = NhaCungCap.MaNCC INNER JOIN
                         NhanVien ON HoaDon.MaNV = NhanVien.MaNV AND GianHang.MaGH = NhanVien.MaGH
WHERE        (ChiTietHoaDon.MaHD IN
                             (SELECT        MaHD
                               FROM            HoaDon AS HoaDon_1
                               WHERE        (MaHD = @MaHD)))</CommandText>
